<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="enquiry_thanks" model="website.page">
        <field name="name">Thanks</field>
        <field name="type">qweb</field>
        <field name="url">/enquiry-thank-you</field>
        <field name="website_published">True</field>
        <field name="key">client_management.enquiry_thanks</field>
        <field name="arch" type="xml">
            <t name="Thanks" t-name="client_management.enquiry_thanks">
                <t t-call="website.layout">
                    <div id="wrap">
                        <div class="oe_structure" id="oe_structure_website_crm_contact_us_thanks_1"/>
                        <div class="container">
                            <h1>Thanks!</h1>
                            <div class="row">
                                <div class="col-lg-8">
                                    <div class="alert alert-success" role="status">
                                        Thank you for your enquiry, an email will be sent to you shortly
                                        <button type="button" class="close" data-dismiss="alert">&amp;times;</button>
                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <t t-call="website.company_description"/>
                                </div>
                            </div>
                        </div>
                    </div>
                </t>
            </t>
        </field>
    </record>

    <template id="duplicate_enquiry" name="Already Registered">
        <t t-call="website.layout">
            <div class="container">
                <div class="row mt64">
                    <div class="col-md-10 col-md-offset-1">
                        <div class="alert alert-success" role="status">
                            <h4 class="text-center">You already have applied with provided details.</h4>
                            <p>You can login with provided email id.</p>
                            <p>Contact us if you are having any trouble with login.</p>
                            <p class="text-center">Thank you.</p>
                        </div>
                    </div>
                </div>
            </div>
        </t>
    </template>

    <template id="register_form" name="Register Form">
        <t t-call="website.layout">
            <div class="container">
                <div class="container bg_gray">
                    <div class="row pt_30 pl_24_sm">
                        <div class="col-md-7 col-md-offset-1">
                            <h2>Youth Network</h2>
                            <p class="fs_16">Join our portal and gain access to a world of opportunities</p>
                        </div>
                        <div class="col-md-4">
                            <a href="/register-youth" class="btn btn-primary mt_34" role="button">Youth
                                Enquiry/Registration
                                <i class='fa fa-angle-right fs_18_bold'></i>
                            </a>
                        </div>
                    </div>
                    <div class="row pt_30 pb_30 pl_24_sm">
                        <div class="col-md-7 col-md-offset-1">
                            <h2>Partner Network</h2>
                            <p class="fs_16">Join our network and help provide services to young people.<br/>Are you a
                                Service Provider,Donor or simply a company with<br/>opportunities for young people.
                            </p>
                        </div>
                        <div class="col-md-4">
                            <a href="/register-partner" class="btn btn-primary mt_34" role="button">Partner
                                Enquiry/Registration
                                <i class='fa fa-angle-right fs_18_bold'></i>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </t>
    </template>

