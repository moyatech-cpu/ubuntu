<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
   	<record id="view_bbbee_certificate_view" model="ir.ui.view">
		<field name="name">BBBEE Certificate</field>
        <field name="model">bbbee.certificate</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
           <form string="Users">
           	<header>
           	
           	</header>
           	<sheet>
           		<group>
				<div class="col-md-6 oe_title">
                      <label for="UniqueRegistrationReferenceNumber" string="Serial #"/>
                      <h1>
                          <field name="supplier_identification" readonly="1"/>
                      </h1>
                </div>
                </group>
                <group>
                <field name="user_id" invisible="True"/>
<field name="IsActive"/>
<field name="LastVerficationDate"/>
<field name="EditDate"/>
<field name="CertificateTypeCode"/>
<field name="VerificationRegulatorCode"/>
<field name="StatusLevelOfContributorCode"/>
<field name="CertificateSignedByvalue"/>
<field name="SectorCharterCode"/>
<field name="SubSectorCharterCode"/>
<field name="IRBARegisteredAuditorCode"/>
<field name="SANASAccreditedAgencyCode"/>
<field name="CertificateNumber"/>
<field name="CertificateIssueDate"/>
<field name="CertificateExpiryDate"/>
<field name="BlackOwnership"/>
<field name="BlackWomanOwnership"/>
<field name="IsAcceptUnderstandAffidavid"/>
<field name="CertificateSignedBy"/>
<field name="CertificateSignDate"/>
<field name="ValueAddingSupplier"/>
<field name="EmpoweringSupplier"/>
<field name="OwnershipScore"/>
<field name="ManagementControlScore"/>
<field name="EmploymentEquityScore"/>
<field name="SkillsDevelopmentScore"/>
<field name="PreferentialProcurementScore"/>
<field name="EnterpriseDevelopmentScore"/>
<field name="SocioEconomicDevelopmentScore"/>
<field name="EnterpriseSupplierDevelopmentScore"/>
<field name="LandOwnershipScore"/>
<field name="EmpowermentFinancingScore"/>
<field name="AccessFinancialServicesScore"/>
<field name="EconomicDevelopmentScore"/>
<field name="ForeignOwnershipScore"/>
<field name="StatusTypeCode"/>
<field name="LastVerificationDate"/>
<field name="EditDate"/>
</group>
           	</sheet>
          </form> 
        </field>
    </record>
    <record id="view_bbbee_certificate_tree" model="ir.ui.view">
        <field name="name">BBBEE Certificate</field>
        <field name="model">bbbee.certificate</field>
        <field name="arch" type="xml">
            <tree string="BBBEE Certificate">
<field name="IsActive"/>
<field name="LastVerficationDate"/>
<field name="EditDate"/>
<field name="CertificateTypeCode"/>
<field name="VerificationRegulatorCode"/>
<field name="StatusLevelOfContributorCode"/>
<field name="CertificateSignedByvalue"/>
<field name="SectorCharterCode"/>
<field name="SubSectorCharterCode"/>
<field name="IRBARegisteredAuditorCode"/>
<field name="SANASAccreditedAgencyCode"/>
<field name="CertificateNumber"/>
<field name="CertificateIssueDate"/>
<field name="CertificateExpiryDate"/>
<field name="BlackOwnership"/>
<field name="BlackWomanOwnership"/>
<field name="IsAcceptUnderstandAffidavid"/>
<field name="CertificateSignedBy"/>
<field name="CertificateSignDate"/>
<field name="ValueAddingSupplier"/>
<field name="EmpoweringSupplier"/>
<field name="OwnershipScore"/>
<field name="ManagementControlScore"/>
<field name="EmploymentEquityScore"/>
<field name="SkillsDevelopmentScore"/>
<field name="PreferentialProcurementScore"/>
<field name="EnterpriseDevelopmentScore"/>
<field name="SocioEconomicDevelopmentScore"/>
<field name="EnterpriseSupplierDevelopmentScore"/>
<field name="LandOwnershipScore"/>
<field name="EmpowermentFinancingScore"/>
<field name="AccessFinancialServicesScore"/>
<field name="EconomicDevelopmentScore"/>
<field name="ForeignOwnershipScore"/>
<field name="StatusTypeCode"/>
<field name="LastVerificationDate"/>
<field name="EditDate"/>

        	</tree>
        </field>
    </record>
    
    <record id="action_bbbee_certificate" model="ir.actions.act_window">
        <field name="name">BBBEE Certificate</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">bbbee.certificate</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="domain"></field>
    </record>
	
	<menuitem name="BBBEE Certificate" id="csd_integration.bbbee_certificate_menu" action="action_bbbee_certificate" parent="csd_integration.supplier_details_menu" groups="base.group_system"/>
    
</odoo>