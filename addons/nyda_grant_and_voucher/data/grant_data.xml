<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Grant Application -->
        <record id="sequence_grant_application" model="ir.sequence">
            <field name="name">Grant Application Sequence</field>
            <field name="code">grant.application</field>
            <field name="prefix">GAP</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Sequence for Voucher Application -->
        <record id="sequence_voucher_application" model="ir.sequence">
            <field name="name">Voucher Application Sequence</field>
            <field name="code">voucher.application</field>
            <field name="prefix">VAP</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Sequence for Voucher Isurance -->
        <record id="sequence_voucher_isurance" model="ir.sequence">
            <field name="name">Voucher Isurance Sequence</field>
            <field name="code">voucher.isurance</field>
            <field name="prefix">VI</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>

