    <!--Voucher Status Report Form View-->
    <record model="ir.ui.view" id="voucher_status_report_bdo_form_view">
        <field name="name">voucher.status.report.bdo.form.view</field>
        <field name="model">voucher.status.report.bdo</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="start_date" required="1"/>
                    </group>
                    <group>
                        <field name="end_date" required="1"/>
                    </group>
                </group>
                <group>
                    <field name="type" invisible="1"/>
                    <field name="all_branch" attrs="{'invisible': [('type', 'in', ['status_branch', 'status_branch_cons'])]}"/>
                    <field name="branch_id" options="{'no_open': True, 'no_create': True}" attrs="{'required': [('all_branch', '=', False)], 'invisible': [('type', 'in', ['status_branch', 'status_branch_cons'])]}"/>
                    <field name="type" invisible="0"/>
                    <!-- <field name="x_bda" invisible="0"/> -->
                    <field name="report_type" invisible="0" attrs="{'invisible': [('type', 'in', ['all', 'received_applications', 'approved_applications', 'declined_applications'])]}"/>
                </group>
                <footer>
                    <button string="Download Received Applications Report" name="get_status_report" type="object" class="btn-primary" attrs="{'invisible': [('type', '!=', 'received_applications')]}"/>
                    <button string="Download Approved Applications Report" name="get_status_report" type="object" class="btn-primary" attrs="{'invisible': [('type', '!=', 'approved_applications')]}"/>
                    <button string="Download Declined Applications Report" name="get_status_report" type="object" class="btn-primary" attrs="{'invisible': [('type', '!=', 'declined_applications')]}"/>
                    
                    <button string="Download Applications Report" name="get_status_report" type="object" class="btn-primary" attrs="{'invisible': [('type', '!=', 'all')]}"/>
                    <button string="Download BDO Report" name="get_status_report" type="object" class="btn-primary" attrs="{'invisible': [('type', '!=', 'status_bdo')]}"/>
                    
                    <button string="Download Status Per Branch Report" name="get_status_report" type="object" class="btn-primary" attrs="{'invisible': [('type', '!=', 'status_branch')]}"/>
                    <button string="Download Status Per Branch Report(Consolidated)" name="get_status_report" type="object" class="btn-primary" attrs="{'invisible': [('type', '!=', 'status_branch_cons')]}"/>
                    <button string="Download Status Per Disabled Report" name="get_status_report" type="object" class="btn-primary" attrs="{'invisible': [('type', '!=', 'status_disable')]}"/>
                    <button string="Download Status Per Gender Report" name="get_status_report" type="object" class="btn-primary" attrs="{'invisible': [('type', '!=', 'status_gender')]}"/>
                    <button string="Download Status Per Race Report" name="get_status_report" type="object" class="btn-primary" attrs="{'invisible': [('type', '!=', 'status_race')]}"/>
                    <button string="Download Status Per Rural/Urban Report" name="get_status_report" type="object" class="btn-primary" attrs="{'invisible': [('type', '!=', 'status_rural_urban')]}"/>
                    <button string="Download Status Per Service Provider Report" name="get_status_report" type="object" class="btn-primary" attrs="{'invisible': [('type', '!=', 'status_sp')]}"/>
                    <button string="Download Status Per Service Report" name="get_status_report" type="object" class="btn-primary" attrs="{'invisible': [('type', '!=', 'status_service')]}"/>
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        
        </field>
    </record>
